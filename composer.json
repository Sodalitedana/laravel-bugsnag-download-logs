{
    "name": "sodalitedana/laravel-bugsnag-download-logs",
    "description": "This is my package laravel-bugsnag-download-logs",
    "keywords": [
        "Sodalitedana",
        "laravel",
        "laravel-bugsnag-download-logs"
    ],
    "homepage": "https://github.com/sodalitedana/laravel-bugsnag-download-logs",
    "license": "MIT",
    "authors": [
        {
            "name": "Sodalitedana",
            "email": "sodalite.dana@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0||^12.0",
        "nesbot/carbon": "^3.0|^2.0",
        "laravel/prompts": "^0.3|^0.4"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Sodalitedana\\LaravelBugsnagDownloadLogs\\": "src/",
            "Sodalitedana\\LaravelBugsnagDownloadLogs\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sodalitedana\\LaravelBugsnagDownloadLogs\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sodalitedana\\LaravelBugsnagDownloadLogs\\LaravelBugsnagDownloadLogsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
